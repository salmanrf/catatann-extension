{
  "manifest_version": 3,
  "name": "Catatann",
  "version": "1.0",
  "description": "Create clean looking notes quickly with markdown, anywhere in the browser.",
  "content_scripts": [
    {
      "js": ["dist/content.js"],
      "matches": ["http://*/*", "https://*/*"]
    }
  ],
  "background": {
    "service_worker": "dist/worker.js"
  }
}
